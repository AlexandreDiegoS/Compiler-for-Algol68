# Inicio do programa #
BEGIN
  	PROC mod = (INT x, y) INT:
  	BEGIN
  		INT result;
  		result := x;
  		WHILE result > y
		DO
			result := result - y;
		OD
		# INT x; # # Descomentar para teste da regra 10 # 
  		RETURN result;
  	END
  	
  	PROC main = () VOID:
  	BEGIN
  		INT a, b;
  		a := 5;
  		b := 15;
  		
  		PRINT(mod(a,b));  
  	END
END